        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Numeros enteros Random" enabled="true">
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="script">import java.util.Random;

// Genera una cadena de texto de longitud deseada
int longitud = 10; // Cambiar este valor según la longitud deseada
StringBuilder sb = new StringBuilder();
Random random = new Random();
for (int i = 0; i &lt; longitud; i++) {
    sb.append(random.nextInt(10)); // Genera un número entero aleatorio del 0 al 9 y lo agrega a la cadena
}

// Almacena la cadena generada en una variable
String numerosEnteros = sb.toString();
vars.put(&quot;numerosEnteros&quot;, numerosEnteros);
</stringProp>
          <stringProp name="scriptLanguage">groovy</stringProp>
        </JSR223Sampler>
